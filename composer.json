    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "yiisoft/yii2",
                "version": "0.0.5",
                "source": {
                    "url": "https://github.com/yiisoft/yii2.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "yiiext/yiimetroui",
                "version": "0.0.1",
                "authors": [
                    {
                        "name": "Philipp Frenzel",
                        "homepage": "http://frenzel.net"
                    }
                ],
                "source": {
                    "url": "https://github.com/philippfrenzel/yiimetroui.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type":"package",
            "package":{
                "name":"yiiext/assetparser",
                "version":"1.0.7",
                "authors": [
                    {
                        "name": "Michel Bobilier",
                        "homepage": "https://github.com/athos99"
                    }
                ],
                "require": {
                    "php": ">=5.2.0"
                },
                "source":{
                    "url":"https://github.com/athos99/assetparser.git",
                    "type":"git",
                    "reference":"master"
                }
            }
        },
        {
            "type": "composer",
            "url": "http://packages.phundament.com"
        }
    ],
    "config": {
        "vendor-dir": "yii/vendor"
    },
    "bin": [
        "yii/yiic"
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.0",
        "michelf/php-markdown": "1.3",
        "twig/twig": "1.12.*",
        "smarty/smarty": "3.1.*",
        "ezyang/htmlpurifier": "v4.5.0",
        "yiisoft/yii2": ">0.0.2",
        "yiiext/assetparser":"1.0.7",
        "yiiext/yiimetroui":">0.0.0"         
    }
}